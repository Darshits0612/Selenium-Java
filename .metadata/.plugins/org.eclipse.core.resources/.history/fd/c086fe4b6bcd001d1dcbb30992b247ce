package programing;

import java.util.Scanner;

public class multiplication {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int sum=1;
		for(int i=1;i<=10;i++)
		{
		  sum=a*i;
		  System.out.println(a+"*"+i" ="+sum);
		}
		
	}

}
